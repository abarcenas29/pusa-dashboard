{
  "name": "react-sandbox-v2",
  "version": "0.0.1",
  "description": "Updated version of the React-Sandbox with updated packages and methods",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build:prod": "rimraf ./build/* && cross-env NODE_ENV=production && webpack --config internals/webpack.config.js --mode production --progress",
    "postinstall": "rimraf node_modules/jest-runtime/node_modules/babel-core node_modules/jest-config/node_modules/babel-core",
    "start:dev": "cross-env NODE_ENV=development && rimraf ./build/* && webpack-dev-server --config internals/webpack.config.js --mode development --open --hot",
    "test": "cross-env NODE_ENV=test && jest --colors"
  },
  "dependencies": {
    "@abarcenas/atomic-styles": "^1.0.1",
    "classnames": "^2.2.6",
    "dayjs": "^1.8.16",
    "final-form": "^4.18.5",
    "leaflet": "^1.5.1",
    "react": "^16.9.0",
    "react-cropper": "^1.2.0",
    "react-dom": "^16.9.0",
    "react-final-form": "^6.3.0",
    "react-helmet": "^5.2.1",
    "react-intl": "^3.1.11",
    "react-leaflet": "^2.4.0",
    "react-redux": "^7.1.0",
    "react-redux-toastr": "^7.5.1",
    "react-toastify": "^5.4.0",
    "react-webcam": "^3.1.1",
    "redux": "^4.0.4",
    "redux-actions": "^2.6.5",
    "redux-observable": "^1.1.0",
    "reselect": "^4.0.0",
    "rxjs": "^6.5.2",
    "sanitize.css": "^11.0.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.5.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@hot-loader/react-dom": "^16.9.0",
    "autoprefixer": "^9.6.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^5.2.0",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
    "dotenv": "^8.1.0",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^6.2.2",
    "eslint-config-standard": "^14.0.1",
    "eslint-config-standard-react": "^9.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.18.2",
    "react-hot-loader": "^4.12.11",
    "react-loadable": "^5.5.0",
    "react-router-dom": "^5.0.1",
    "redux-devtools-extension": "^2.13.8",
    "rimraf": "^3.0.0",
    "sass-loader": "^7.3.1",
    "style-loader": "^1.0.0",
    "styled-components": "^4.3.2",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0",
    "workbox-webpack-plugin": "3.6.3"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-syntax-dynamic-import"
    ],
    "env": {
      "development": {
        "plugins": [
          "react-hot-loader/babel"
        ]
      },
      "production": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "modules": false
            }
          ]
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "standard",
      "standard-react"
    ],
    "plugins": [
      "react"
    ],
    "globals": {
      "Blob": true,
      "DEV_MARKER": true,
      "FileReader": true,
      "FormData": true,
      "confirm": true,
      "localStorage": true,
      "location": true,
      "sessionStorage": true,
      "File": true,
      "atob": true
    },
    "rules": {
      "react/jsx-fragments": 0,
      "react/prop-types": 0,
      "camelcase": [
        0,
        {
          "ignoreDestructuring": true
        }
      ]
    }
  },
  "sideEffects": false,
  "jest": {
    "verbose": true,
    "moduleNameMapper": {
      "Components(.*)$": "<rootDir>/app/components$1",
      "Containers(.*)$": "<rootDir>/app/containers$1",
      "Helpers(.*)$": "<rootDir>/app/helpers$1",
      "Modules(.*)$": "<rootDir>/app/modules$1",
      "Styles(.*)$": "<rootDir>/app/styles$1",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQute": true,
    "trailingComma": "none"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  }
}
